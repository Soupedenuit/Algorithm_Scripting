function largestOfFour(arr) {
  // You can do this!
  var arr2 = [];
  for ( var j=0; j < arr.length; j++) {
    var maxNum;
    var newMaxNum = 0;
    for ( var i=0; i < arr.length; i++) {
    maxNum = arr[j][i];
      if (maxNum > newMaxNum) {
        newMaxNum = maxNum;
      }
}
  arr2.push(newMaxNum);
  }
  return arr2;
  

}
largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
